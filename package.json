{
  "name": "babel-plugin-react-source-string",
  "version": "1.1.4",
  "description": "adds data-source attribute to html elements with react components source line of code",
  "author": "Jaroslav Barov jaroslav.barov@gmail.com",
  "license": "MIT",
  "keywords": [
    "babel",
    "plugin",
    "react",
    "source",
    "data-source"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "test:ci": "jest",
    "test": "npm run build; jest"
  },
  "repository": "git@github.com:tanchu/babel-plugin-react-source-string.git",
  "dependencies": {
    "@babel/core": "^7.28.3",
    "@babel/types": "^7.28.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/traverse": "^7.28.3",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.2.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "unbuild": "^1.2.1"
  }
}
